{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": [
    "plugin:react/recommended",
    "airbnb"
  ],
  "parserOptions": {
    "ecmaFeatures": {
      "jsx": true
    },
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "plugins": [
    "react"
  ],
  "rules": {
    "no-undef": "off"  // 이 줄은 `no-undef` 오류를 끄기 위해 추가합니다.
  },
  "globals": {
    "naver": "readonly"
  }
}
